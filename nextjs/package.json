{
  "private": true,
  "scripts": {
    "start": "concurrently \"cd next1; yarn dev \" \"cd next2; yarn dev\"",
    "build": "lerna run --scope @nested/* build",
    "serve": "lerna run --scope @nested/* --parallel serve",
    "clean": "lerna run --scope @nested/* --parallel clean",
    "postinstall": "concurrently \"cd ./next1 && yarn\" \"cd next2 && yarn \""
  },
  "dependencies": {
    "concurrently": "^5.2.0"
  }
}
